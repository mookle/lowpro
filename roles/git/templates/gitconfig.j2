[user]
    name = mookle
    email = {{ git_user_email }}
[core]
    editor = vim
[color]
    ui = auto
[color "status"]
    added = green
    changed = yellow
    untracked = normal
[github]
    user = mookle
[alias]
    prune = fetch --prune
[merge]
    ff = only
[push]
    followTags = true
[status]
    statusUntrackedFiles = all
